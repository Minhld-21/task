{
  "name": "phuckhangshop",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "clean:watchman": "watchman watch-del-all",
    "start:reset": "npm start -- --reset-cache",
    "clean": "rm -rf node_modules && rm -rf package-lock.json",
    "clean:android": "cd android && ./gradlew clean",
    "clean:ios": "rm -rf ios/Pods && rm -rf ios/Podfile.lock",
    "setup:ios": "cd ios && pod install && cd ..",
    "android": "react-native run-android --variant=developmentdebug",
    "android:release": "react-native run-android --variant=developmentrelease",
    "android:prod": "react-native run-android --variant=productiondebug",
    "android:prod-release": "react-native run-android --variant=productionrelease",
    "android:aab": "cd android && ./gradlew clean && ENVFILE=.env.production ./gradlew bundleRelease",
    "ios": "react-native run-ios --scheme 'phuckhangshop' --simulator='iPhone 14'",
    "ios:prod": "react-native run-ios --scheme 'phuckhangshop_Prod' --simulator='iPhone 14'",
    "test": "jest",
    "lint": "eslint ."
  },
